<?xml version="1.0" ?>
<xs:schema id="History" targetNamespace="http://www.rainbowportal.net/History.xsd" xmlns:mstns="http://www.rainbowportal.net/History.xsd" xmlns="http://www.rainbowportal.net/History.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="History" msdata:IsDataSet="true" msdata:Locale="it-IT" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Release">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" type="xs:int" minOccurs="0" />
							<xs:element name="Version" type="xs:string" minOccurs="0" />
							<xs:element name="Script" type="xs:string" minOccurs="0" />
							<xs:element name="Date" type="xs:date" minOccurs="0" />
							<xs:element name="Modules">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Installer" type="xs:string" minOccurs="0" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
